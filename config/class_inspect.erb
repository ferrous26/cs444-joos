<%= green '@' %><%= package.fully_qualified_name.map { |id| cyan id }.join('.') %>

<%= inspect_modifiers_space %>class <%= cyan name.value %> < <%= superclass.inspect %> (<%= interfaces.map(&:inspect).join(', ') %>)

<%= fields.map { |f| f.inspect }.join("\n\n") << "\n\n" %>
<%= methods.map { |m| m.inspect }.join("\n\n") << "\n\n" %>
<%= constructors.map { |c| c.inspect }.join("\n\n") << "\n\n" %>
